    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.HtmlRenderPreformatted</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Render the given text as preformatted text in html.</description>
        <name>HtmlRenderPreformatted</name>
        <script><![CDATA[function HtmlRenderPreformatted(strText)
{
	strText = GetStringValue(strText);
	if (strText == '')
	{
		return '';
	}
	return '\n<pre>\n'+HtmlEncode(strText)+'\n</pre>\n';
}]]></script>

        <name>x_snc_devtools.GetIntegerValue</name>
        <script><![CDATA[function GetIntegerValue(vValue)
{
	if (vValue === true)
	{
		return 1;
	}
	var vResult = parseInt(vValue);
	if (isNaN(vResult))
	{
		return 0;
	}
	return vResult;
}]]></script>

<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Returns true if the given parameter is an integer value, returns false if not.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.IsInteger</name>
        <script><![CDATA[function IsInteger(vValue)
{
	if (GetType(vValue) != 'number')
	{
		return false;
	}
	var vIntValue = parseInt(vValue);
	if (vIntValue === vValue)
	{
		return true;
	}
	return false;
}]]></script>
        <script_name>IsInteger</script_name>

<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Appends the second given array to the first.</description>
        <global>true</global>
        <name>x_snc_devtools.ArrayAppend</name>
        <script><![CDATA[function ArrayAppend(vArray1, vArray2)
{
	var aResult = [];
	if (IsArray(vArray1) == true)
	{
		aResult = vArray1;
	}
	if (IsArray(vArray2) == false)
	{
		return aResult;
	}
	aResult = aResult.concat(vArray2);
	return aResult;
}]]></script>
        <script_name>ArrayAppend</script_name>

<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_script_include">
    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.RedirectToRecord</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Redirects to the form view of the given record if it is valid. </description>
        <name>RedirectToRecord</name>
        <script><![CDATA[function RedirectToRecord(record,strView)
{
	if (IsValidRecord(record) == false)
	{
		return false;
	}
	strView = GetStringValue(strView);
	var strViewParam = '';
	if (strView != '')
	{
		strViewParam = '&sysparm_view='+strView;
	}
	gs.setRedirect(record.getRecordClassName()+'.do?sys_id='+record.sys_id+strViewParam);
	return true;
}]]></script>
